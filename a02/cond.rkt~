#reader(lib "htdp-beginner-reader.ss" "lang")((modname cond) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor mixed-fraction #f #t none #f () #t)))
(define (q3a n a?)
  (cond [a? (cond [(>= n 0) (+ n 1)]
                  [else (- n 1)])]
        [else 0]))